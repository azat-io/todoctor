{
  "extends": "@azat-io/typescript-config",
  "include": ["**/*.svelte", "**/*.ts"],
  "compilerOptions": {
    "paths": {
      "~/elements/*": ["./preview/elements/*"],
      "~/typings/*": ["./preview/typings/*"],
      "~/package.json": ["./package.json"],
      "~/blocks/*": ["./preview/blocks/*"],
      "~/styles/*": ["./preview/styles/*"],
      "~/stores/*": ["./preview/stores/*"],
      "~/view/*": ["./preview/pages/*"]
    }
  }
}
